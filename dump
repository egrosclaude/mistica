PRAGMA foreign_keys=OFF;
BEGIN TRANSACTION;
CREATE TABLE insumos (
	iid INTEGER NOT NULL, 
	descr VARCHAR NOT NULL, 
	cost FLOAT, 
	cant FLOAT, 
	unid VARCHAR, 
	PRIMARY KEY (iid), 
	UNIQUE (descr)
);
